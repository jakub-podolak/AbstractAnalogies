#!/bin/bash
#SBATCH --partition=gpu
#SBATCH --gpus=1
#SBATCH --job-name=analogy-eval
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=18
#SBATCH --time=04:00:00
#SBATCH --output=./outfiles/analogy_eval_%j.out

module purge
module load 2022
module load Anaconda3/2022.05

cd $HOME/AbstractAnalogies/eval

export PYTHONPATH=$PYTHONPATH:~/AbstractAnalogies
conda activate abstract
git checkout sy/verbal-analogy

# Running the Python script
python eval_verbal_analogy.py --model mistral7b --task verbal_analogies
